{
  "security.workspace.trust.untrustedFiles": "open",
  "editor.formatOnSave": true,
  "files.insertFinalNewline": false,
  "editor.renderWhitespace": "all",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "gitlens.views.branches.files.compact": false,
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "editor.fontSize": 13,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Menlo for Powerline",

  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Default Light+",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.hlsearch": true,
  "vim.smartRelativeLine": true,
  "vim.gdefault": false,
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["d"],
      "after": ["\"", "0", "d"]
    },
    {
      "before": ["<Leader>", "d"],
      "after": ["d"]
    },
    {
      "before": ["c"],
      "after": ["\"", "0", "c"]
    },
    {
      "before": ["<Leader>", "c"],
      "after": ["c"]
    },
    {
      "before": ["<Leader>", "j"],
      "after": ["J"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["<Leader>", "t", "t"],
      "commands": [":tabnew"]
    },
    {
      "before": ["<Leader>", "t", "n"],
      "commands": [":tabnext"]
    },
    {
      "before": ["<Leader>", "t", "p"],
      "commands": [":tabprev"]
    },
    {
      "before": ["<Leader>", "t", "o"],
      "commands": [":tabo"]
    },
    {
      "before": ["<Leader>", "/"],
      "commands": [":noh"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<Leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<Leader>", "o"],
      "after": ["", "<Esc>"]
    },
    // {
    //   "before": ["<Leader>", "o"],
    //   "after": ["o", "<Esc>"]
    // },
    {
      "before": ["<Leader>", "O"],
      "after": ["O", "<Esc>"]
    },
    {
      "before": ["<C-n>"],
      "after": [],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["d"],
      "after": ["\"", "0", "d"]
    },
    {
      "before": ["<Leader>", "d"],
      "after": ["d"]
    },
    {
      "before": ["c"],
      "after": ["\"", "0", "c"]
    },
    {
      "before": ["<Leader>", "c"],
      "after": ["c"]
    },
    {
      "before": ["p"],
      "after": ["\"", "0", "d", "P"]
    }
  ],

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
